{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "SeqServerUrl": null,
    "LogstashgUrl": null,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Inventory.Configuration.Api": "Information",
        "System": "Warning",
        "Elastic.Apm": "Debug"
      }
    }
  },
  "HttpHostingConfiguration": {
    "UseReverseProxy": true,
    "ProxyBasePath": "configuration",
    "ProxyScheme": "http",
    "EnableHttpHeadersLogging":  true
  }
}
